{
  "datasets": [
    {
      "name": "86c5dfb1",
      "displayName": "Gold_Summary_Data",
      "queryLines": [
        "-- This query gathers all the \"Intelligence\" from our Gold layer\n",
        "SELECT \n",
        "    customer_id, \n",
        "    amount, \n",
        "    dist_from_home_km, \n",
        "    fraud_score,\n",
        "    event_time\n",
        "FROM fraud_sentinel_catalog.detection_service.gold_transaction_summary"
      ]
    },
    {
      "name": "a77a9977",
      "displayName": "Counter",
      "queryLines": [
        "-- Executive KPI Summary\n",
        "\n",
        "WITH last_24h AS (\n",
        "    SELECT *\n",
        "    FROM fraud_sentinel_catalog.detection_service.gold_fraud_alerts\n",
        "    WHERE event_time > now() - interval 24 hours\n",
        "),\n",
        "\n",
        "prev_24h AS (\n",
        "    SELECT COUNT(*) AS prev_alerts\n",
        "    FROM fraud_sentinel_catalog.detection_service.gold_fraud_alerts\n",
        "    WHERE event_time BETWEEN \n",
        "        now() - interval 48 hours \n",
        "        AND now() - interval 24 hours\n",
        ")\n",
        "\n",
        "SELECT\n",
        "    COUNT(*) AS total_alerts,\n",
        "    ROUND(SUM(amount), 2) AS total_exposure,\n",
        "    COUNT(DISTINCT customer_id) AS impacted_customers,\n",
        "    ROUND(\n",
        "        (\n",
        "            (COUNT(*) - (SELECT prev_alerts FROM prev_24h))\n",
        "            / NULLIF((SELECT prev_alerts FROM prev_24h), 0)\n",
        "        ) * 100,\n",
        "        2\n",
        "    ) AS pct_change_vs_yesterday\n",
        "FROM last_24h"
      ]
    },
    {
      "name": "d9814475",
      "displayName": "Action List",
      "queryLines": [
        "-- Identifying customers with the highest total flagged amount\n",
        "SELECT \n",
        "    customer_id, \n",
        "    sum(amount) as total_flagged_value \n",
        "FROM fraud_sentinel_catalog.detection_service.gold_fraud_alerts\n",
        "GROUP BY customer_id\n",
        "ORDER BY total_flagged_value DESC\n",
        "LIMIT 10"
      ]
    },
    {
      "name": "e0367dc5",
      "displayName": "Fraud Distribution",
      "queryLines": [
        "-- Breaking down transactions by their Risk Category\n",
        "-- Fact (Transactions) classified by the Scoring logic\n",
        "SELECT \n",
        "    CASE \n",
        "        WHEN fraud_score >= 70 THEN 'High Risk (Fraud)'\n",
        "        WHEN fraud_score >= 30 THEN 'Medium Risk (Warning)'\n",
        "        ELSE 'Low Risk (Safe)'\n",
        "    END AS risk_level,\n",
        "    count(*) as transaction_count\n",
        "FROM fraud_sentinel_catalog.detection_service.gold_transaction_summary\n",
        "GROUP BY 1"
      ]
    },
    {
      "name": "dcc89a20",
      "displayName": "High-Risk Alerts (24h)",
      "queryLines": [
        "SELECT \n",
        "  COUNT(*) AS total_alerts\n",
        "FROM fraud_sentinel_catalog.detection_service.gold_fraud_alerts\n",
        "WHERE event_time > now() - interval 24 hours"
      ]
    },
    {
      "name": "dec705ca",
      "displayName": "Total Exposure (24h)",
      "queryLines": [
        "SELECT \n",
        "  CONCAT('R ', FORMAT_NUMBER(SUM(amount), 0)) AS total_exposure\n",
        "FROM fraud_sentinel_catalog.detection_service.gold_fraud_alerts\n",
        "WHERE event_time > now() - interval 24 hours"
      ]
    },
    {
      "name": "5a852fd2",
      "displayName": "Customer Impacted",
      "queryLines": [
        "SELECT \n",
        "  COUNT(DISTINCT customer_id) AS impacted_customers\n",
        "FROM fraud_sentinel_catalog.detection_service.gold_fraud_alerts\n",
        "WHERE event_time > now() - interval 24 hours"
      ]
    },
    {
      "name": "3eaa816a",
      "displayName": "Provincial Fraud Exposure and Alert Summary",
      "queryLines": [
        "SELECT \n",
        "  home_province,\n",
        "  ROUND(SUM(amount), 2) AS total_exposure,\n",
        "  COUNT(*) AS alert_count\n",
        "FROM fraud_sentinel_catalog.detection_service.gold_fraud_alerts\n",
        "WHERE event_time > now() - interval 24 hours\n",
        "GROUP BY home_province\n",
        "ORDER BY total_exposure DESC"
      ]
    },
    {
      "name": "0cd320da",
      "displayName": "Gold_Alerts_24h_Base",
      "queryLines": [
        "SELECT\n",
        "    event_time,\n",
        "    customer_id,\n",
        "    home_province,\n",
        "    amount,\n",
        "    fraud_score,\n",
        "    dist_from_home_km,\n",
        "    CASE \n",
        "        WHEN fraud_score >= 70 THEN 'High Risk (Fraud)'\n",
        "        WHEN fraud_score >= 30 THEN 'Medium Risk (Warning)'\n",
        "        ELSE 'Low Risk (Safe)'\n",
        "    END AS risk_level\n",
        "FROM fraud_sentinel_catalog.detection_service.gold_fraud_alerts\n",
        "WHERE event_time > now() - interval 24 hours"
      ]
    }
  ],
